<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wzt.tapm.mapper.UserMapper">
    <select id="selectUserByAccount" resultType="com.wzt.tapm.entity.UserBean" parameterType="String">
        SELECT * FROM user_info where account = #{account}
    </select>

    <insert id="insertUser" parameterType="com.wzt.tapm.entity.UserBean">
        insert into user_info(account,password,email,status) values (#{account},#{password},#{email},#{status})
    </insert>
</mapper>